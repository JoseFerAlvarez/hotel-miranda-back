{
    "trailingSlash": "auto",
    "routes": [
        {
            "route": "/",
            "methods": [
                "PUT",
                "POST",
                "PATCH",
                "DELETE"
            ]
        },
        {
            "route": "/rooms",
            "methods": [
                "PUT",
                "POST",
                "PATCH",
                "DELETE"
            ]
        }
    ],
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/css/*"
        ]
    },
    "responseOverrides": {
        "400": {
            "rewrite": "index.html"
        },
        "401": {
            "redirect": "index.html",
            "statusCode": 302
        },
        "403": {
            "rewrite": "index.html"
        },
        "404": {
            "rewrite": "index.html"
        }
    },
    "globalHeaders": {
        "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
    },
    "mimeTypes": {
        ".json": "text/json"
    }
}
